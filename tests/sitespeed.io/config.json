  "budget": {
    "configPath": "budget.json",
    "output": "junit"
  },
  "screenshot": {
    "type": "jpg"
  },
  "filmstrip": {
    "showAll": false
  },
  "browsertime": {
    "iterations": 1,
    "browser": "chrome",
    "basicAuth": "klant@KlantLogin",
    "visualMetrics": true,
    "video": false
  },
  "html": {
    "showScript": true,
    "summaryBoxes": [
      "timings.SpeedIndex",
      "timings.fullyLoaded",
      "axe.critical",
      "axe.serious",
      "axe.moderate",
      "axe.minor",
      "score.bestpractice",
      "score.privacy",
      "score.performance"
    ],
    "pageSummaryMetrics": [
      "timings.SpeedIndex",
      "timings.fullyLoaded",
      "axe.critical",
      "axe.serious",
      "axe.moderate",
      "axe.minor",
      "score.bestpractice",
      "score.privacy",
      "score.performance"
